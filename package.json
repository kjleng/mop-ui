  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "invalidate-cache": "aws cloudfront create-invalidation",
    "sync-dev": "aws s3 sync build/ s3://dev-merchant-onboarding-ui",
    "deploy-dev": "npm run build && npm run sync-dev && npm run invalidate-cache -- --distribution-id EP1928UHJ9I66 --paths /*",
    "sync-qa": "aws s3 sync build/ s3://qa-merchant-onboarding-ui",
    "deploy-qa": "npm run build && npm run sync-qa && npm run invalidate-cache -- --distribution-id E10H0ZO4HMY97C --paths /*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/material-ui": "^0.21.8"
  }
